{
  "collectionName": "components_elements_contact_form_configs",
  "info": {
    "displayName": "Contact Form Configuration",
    "description": "Configure which fields to show in the contact form and form settings"
  },
  "options": {},
  "attributes": {
    "heading": {
      "type": "string",
      "default": "Get in Touch"
    },
    "description": {
      "type": "text",
      "default": "Ready to start your project? Let's discuss how we can help bring your vision to life."
    },
    "theme": {
      "type": "enumeration",
      "default": "turquoise",
      "enum": [
        "turquoise",
        "orange",
        "black"
      ]
    },
    "fields": {
      "type": "component",
      "component": "elements.form-fields",
      "repeatable": false
    },
    "hubspotFormId": {
      "type": "string",
      "required": false
    },
    "hubspotPortalId": {
      "type": "string",
      "required": false
    },
    "useDefaultFieldConfig": {
      "type": "boolean",
      "default": true
    }
  }
}
