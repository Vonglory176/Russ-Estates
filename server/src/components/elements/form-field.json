{
  "collectionName": "components_elements_form_field",
  "info": {
    "displayName": "Form Field Configuration",
    "description": "Configure individual form field properties"
  },
  "options": {},
  "attributes": {
    "enabled": {
      "type": "boolean",
      "default": true
    },
    "label": {
      "type": "string"
    },
    "placeholder": {
      "type": "string"
    },
    "order": {
      "type": "integer",
      "default": 0
    },
    "fieldType": {
      "type": "enumeration",
      "default": "text",
      "enum": [
        "text",
        "email",
        "tel",
        "textarea"
      ]
    }
  }
}
