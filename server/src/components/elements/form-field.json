{
  "collectionName": "components_elements_form_field",
  "info": {
    "displayName": "Form Field Configuration",
    "description": "Configure individual form field properties"
  },
  "options": {},
  "attributes": {
    "enabled": {
      "type": "boolean",
      "default": true,
      "description": "Show this field in the form"
    },
    "label": {
      "type": "string",
      "description": "Field label displayed to users"
    },
    "placeholder": {
      "type": "string",
      "description": "Placeholder text shown in the input field"
    },
    "order": {
      "type": "integer",
      "default": 0,
      "description": "Display order of the field (lower numbers appear first)"
    },
    "fieldType": {
      "type": "enumeration",
      "enum": [
        "text",
        "email",
        "tel",
        "textarea"
      ],
      "default": "text",
      "description": "Type of input field"
    },
    "validation": {
      "type": "component",
      "component": "elements.field-validation",
      "repeatable": false,
      "description": "Basic validation rules for this field"
    }
  }
}